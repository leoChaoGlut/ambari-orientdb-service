<metainfo>
    <schemaVersion>2.0</schemaVersion>
    <services>
        <service>
            <name>ORIENTDB</name>
            <displayName>OrientDB</displayName>
            <comment></comment>
            <version>3.1.2</version>
            <components>
                <component>
                    <name>ORIENTDB</name>
                    <displayName>OrientDB</displayName>
                    <category>MASTER</category>
                    <cardinality>1+</cardinality>
                    <versionAdvertised>true</versionAdvertised>
                    <commandScript>
                        <script>scripts/orientdb.py</script>
                        <scriptType>PYTHON</scriptType>
                        <timeout>300</timeout>
                    </commandScript>
                </component>
            </components>

            <configuration-dependencies>
                <config-type>automatic-backup.json</config-type>
                <config-type>custom-sql-functions.json</config-type>
                <config-type>default-distributed-db-config.json</config-type>
                <config-type>hazelcast_config</config-type>
                <config-type>jdbc-drivers.json</config-type>
                <config-type>orientdb-client-log.properties</config-type>
                <config-type>orientdb-etl-log.properties</config-type>
                <config-type>orientdb-server-config_config</config-type>
                <config-type>orientdb-server-log.properties</config-type>
                <config-type>security.json</config-type>
            </configuration-dependencies>

        </service>
    </services>
</metainfo>
